{
  "name": "react-todo",
  "version": "1.0.0",
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "clean": "rm -rf static",
    "start": "NODE_ENV=production node ./server/app.js",
    "start:dev": "NODE_ENV=development nodemon ./server/app.js",
    "build:dev": "npm run clean && OUTPUT=./static ./node_modules/.bin/webpack --progress --config webpack.config.dev.js",
    "build:prod": "npm run clean && OUTPUT=./static ./node_modules/.bin/webpack --progress --config webpack.config.prod.js",
    "lint": "./node_modules/.bin/eslint .",
    "lint-only-change": "./node_modules/.bin/eslint",
    "test": "./node_modules/karma/bin/karma start",
    "precommit": "./scripts/pre-commit.sh",
    "prepush": "npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com.ru:fresk-nc/react-todo.git"
  },
  "dependencies": {
    "autoprefixer": "6.3.1",
    "babel-core": "6.5.2",
    "babel-loader": "6.2.1",
    "babel-plugin-transform-object-assign": "6.5.0",
    "babel-preset-es2015": "6.5.0",
    "babel-preset-react": "6.5.0",
    "body-parser": "1.14.2",
    "classnames": "2.2.3",
    "cookie-parser": "1.4.1",
    "css-loader": "0.23.1",
    "express": "4.13.3",
    "extract-text-webpack-plugin": "1.0.1",
    "html-webpack-plugin": "1.7.0",
    "immutable": "3.7.6",
    "keymirror": "0.1.1",
    "mongoose": "4.4.6",
    "morgan": "1.7.0",
    "node-uuid": "1.4.7",
    "postcss-loader": "0.8.0",
    "react": "0.14.7",
    "react-addons-css-transition-group": "0.14.7",
    "react-addons-pure-render-mixin": "0.14.7",
    "react-dom": "0.14.7",
    "react-redux": "4.4.0",
    "redux": "3.3.1",
    "redux-logger": "2.6.1",
    "redux-optimist": "0.0.2",
    "style-loader": "0.13.0",
    "stylus-loader": "1.4.3",
    "webpack": "1.12.9"
  },
  "devDependencies": {
    "chai-immutable": "git://github.com/astorije/chai-immutable#655d1871a004bad95d9adaed45f52ff586a64ea4",
    "eslint": "2.2.0",
    "eslint-plugin-react": "4.0.0",
    "husky": "0.10.2",
    "karma": "0.13.21",
    "karma-babel-preprocessor": "6.0.1",
    "karma-chai-immutable": "^1.0.2",
    "karma-mocha": "0.2.1",
    "karma-phantomjs-launcher": "1.0.0",
    "karma-sinon-chai": "1.1.0",
    "karma-webpack": "1.7.0",
    "mocha": "2.4.5",
    "nodemon": "1.8.1",
    "react-addons-test-utils": "0.14.7",
    "react-hot-loader": "1.3.0",
    "webpack-dev-middleware": "1.6.0",
    "webpack-hot-middleware": "2.10.0"
  }
}
