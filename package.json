{
  "name": "react-todo",
  "version": "1.0.0",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "clean": "rm -rf static",
    "watch": "./node_modules/.bin/webpack-dev-server --inline --hot --config webpack.config.dev.js",
    "start": "NODE_PATH=./server NODE_CONFIG_DIR=./server/config NODE_ENV=production node ./server/app.js",
    "start:dev": "NODE_PATH=./server NODE_CONFIG_DIR=./server/config NODE_ENV=development nodemon ./server/app.js",
    "daemon": "./scripts/daemon.sh",
    "build:dev": "npm run clean && OUTPUT=./static ./node_modules/.bin/webpack --progress --config webpack.config.dev.js --bail",
    "build:prod": "npm run clean && OUTPUT=./static ./node_modules/.bin/webpack --progress --config webpack.config.prod.js --bail",
    "lint": "./node_modules/.bin/eslint .",
    "lint-only-change": "./node_modules/.bin/eslint",
    "test": "./node_modules/karma/bin/karma start",
    "precommit": "./scripts/pre-commit.sh",
    "prepush": "npm run test",
    "heroku-postbuild": "npm run build:prod"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com.ru:fresk-nc/react-todo.git"
  },
  "dependencies": {
    "autoprefixer": "6.3.1",
    "babel-core": "6.16.0",
    "babel-eslint": "6.1.2",
    "babel-loader": "6.2.5",
    "babel-plugin-transform-object-assign": "6.8.0",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-react-optimize": "1.0.1",
    "babel-preset-stage-0": "6.16.0",
    "bundle-loader": "0.5.4",
    "classnames": "2.2.3",
    "config": "1.21.0",
    "css-loader": "0.25.0",
    "extract-text-webpack-plugin": "1.0.1",
    "html-webpack-plugin": "2.22.0",
    "immutable": "3.7.6",
    "intl": "1.1.0",
    "keymirror": "0.1.1",
    "koa": "1.2.4",
    "koa-bodyparser": "2.2.0",
    "koa-favicon": "1.2.1",
    "koa-logger": "1.3.0",
    "koa-router": "5.4.0",
    "koa-static": "2.0.0",
    "mongoose": "4.4.6",
    "node-uuid": "1.4.7",
    "postcss-loader": "0.13.0",
    "ramda": "0.22.1",
    "react": "15.3.1",
    "react-addons-css-transition-group": "15.3.1",
    "react-dom": "15.3.1",
    "react-intl": "2.1.3",
    "react-redux": "4.4.5",
    "redux": "3.5.2",
    "redux-optimist": "0.0.2",
    "style-loader": "0.13.1",
    "stylus": "0.54.5",
    "stylus-loader": "2.3.1",
    "webpack": "1.13.2",
    "whatwg-fetch": "0.11.0"
  },
  "devDependencies": {
    "chai-immutable": "1.6.0",
    "enzyme": "2.4.1",
    "eslint": "3.3.1",
    "eslint-plugin-react": "6.1.2",
    "husky": "0.10.2",
    "json-loader": "0.5.4",
    "karma": "0.13.21",
    "karma-babel-preprocessor": "6.0.1",
    "karma-chai-immutable": "1.0.2",
    "karma-intl-shim": "1.0.3",
    "karma-jsdom-launcher": "4.0.0",
    "karma-mocha": "0.2.1",
    "karma-sinon-chai": "1.1.0",
    "karma-webpack": "1.7.0",
    "mocha": "2.4.5",
    "nodemon": "1.8.1",
    "react-addons-test-utils": "15.3.1",
    "react-hot-loader": "1.3.0",
    "redux-logger": "2.6.1",
    "webpack-dev-server": "1.14.1"
  }
}
